state enemyunderwater // State for allowing monsters to have their own set of 'lungs' underwater, and for effecting Damage from drowning. [also affects monsters in SPPAAAAAAAAAAACE!!! XD]

// If the monster is in a space sector...
getactor[THISACTOR].sectnum tempf getactorvar[tempf].SPACESECTOR? tempe
ifvare tempe 1 // If In Space Sector
{
 ifvare mainmenu 0 ifvarg ENEMYHP 0
 {
  ifvarl ENEMYINVULNTIME 1
  {
   getactor[THISACTOR].picnum tempc
   switch tempc
    case EMPTYTANK
    case TANK
    case SCORPIONTANK
    case ROTATEGUN
    case DRONE
     setvar tempb 0
    break
   
    default
     setvar tempb 1
    break
   endswitch

   ifvare tempb 0 nullop
   else
   {
    // HP Damage
    setvarvar tempd ENEMYMAXHP mulvar tempd 3 divvar tempd 100
    ifvarn ENEMYHASSHIKON? 0 divvar tempd 10
    ifvarg tempd 0 state monsterpainsounds2
    ifvarg ENEMYHP 0
    {
     subvarvar ENEMYHP tempd
     ifvarg tempd 0
     {
      setvarvar SHOWDAMAGE tempd
      setvar SHOWDAMAGETYPE 0
      setvarvar SHOWDAMAGEACTORID THISACTOR
      state showdamage
     }
    }

   }
  }
  else
  {
   sound INVULNITEM3
  }
 }
}

getsector[THISACTOR].lotag ENEMYSECTORLOTAG

ifvare ENEMYSECTORLOTAG 2 // If the monster is underwater...
{
 getactor[THISACTOR].picnum tempc
 switch tempc
  case SHARK
  case ROTATEGUN
  case DRONE
  case EMPTYTANK
  case TANK
  case SCORPIONTANK
  case OCTABRAIN
  case OCTABRAINSTAYPUT
  case NEWBEAST
  case NEWBEASTSTAYPUT
  case NEWBEASTHANG
  case NEWBEASTJUMP
  case BOSS4
  case BOSS4STAYPUT
   setvar tempb 0
  break
   
  default
   setvar tempb 1
  break
 endswitch

 ifvare tempb 0 nullop
 else
 {
  ifvarg ENEMYAIRTANKAMOUNT 0
  {
   ifvarvarl ENEMYAIRTIME ENEMYMAXAIRTIME
   {
    setvarvar TEMPVAR5 ENEMYMAXAIRTIME
    divvar TEMPVAR5 130
    addvarvar ENEMYAIRTIME TEMPVAR5 // This prevents the monster from continuing to drown if they pick up a Scuba Gear with no breath left. Also emulates a 'transfer' of missing air from a scuba tank to the monster.
    subvarvar ENEMYAIRTANKAMOUNT TEMPVAR5 // This prevents the monster from continuing to drown if they pick up a Scuba Gear with no breath left. Also emulates a 'transfer' of missing air from a scuba tank to the monster.
   }
   else ifvarvare ENEMYAIRTIME ENEMYMAXAIRTIME { setvar ENEMYAIRTIMEEXTRA 1000 }
   else ifvarvarg ENEMYAIRTIME ENEMYMAXAIRTIME
   {
    setvarvar TEMPVAR5 ENEMYAIRTIME
    subvarvar TEMPVAR5 ENEMYMAXAIRTIME
    subvarvar ENEMYAIRTIME TEMPVAR5 // For making sure none of the airtank tics are wasted when quick-filling the air bar
    addvarvar ENEMYAIRTANKAMOUNT TEMPVAR5 // For making sure none of the airtank tics are wasted when quick-filling the air bar
    setvar ENEMYAIRTIMEEXTRA 1000
   }
  }

  ifvare ACTORDEATHSWITCHDELAY 0 ifvarl ENEMYINVULNTIME 1 // If the monster has no Invulnerability time, then they need to breathe...
  {
   ifvarl ENEMYAIRTANKAMOUNT 1 // If the monster has no scuba tank energy
   {
    ifvarg ENEMYAIRTIME 0 // If the enemy has airtime left in their lungs
    {
     ifvare mainmenu 0 ifvare FROZEN? 0
     {
      setvar TEMPVAR5 1000
      ifvarn ENEMYHASSHIKON? 0 divvar TEMPVAR5 10
      subvarvar ENEMYAIRTIMEEXTRA TEMPVAR5

      ifvarl ENEMYAIRTIMEEXTRA 1
      {
       subvar ENEMYAIRTIME 1 // Minus air supply by 1 per tic
       addvar ENEMYAIRTIMEEXTRA 1000
      }
     }
     setvar DROWNDMGFACTOR 10
    }
    else // If the monster has <0 airtime left, start the drowning counter
    {
     ifvare mainmenu 0 ifvarg ENEMYHP 0 addvar ENEMYDROWNTIME 1 // Increment Drown meter 1 per tic
     ifvare ENEMYDROWNTIME 8 // if 8 tics of the drown meter count...
     {
      setvarvar ENEMYBASEDROWNDMG ENEMYMAXHP
      randvar ENEMYEXTRADROWNDMG 3
      addvarvar DROWNDMGFACTOR ENEMYEXTRADROWNDMG

      mulvarvar ENEMYBASEDROWNDMG DROWNDMGFACTOR
      divvar ENEMYBASEDROWNDMG 1000

      setvarvar ENEMYTOTALDROWNDMG ENEMYBASEDROWNDMG
      addvarvar ENEMYTOTALDROWNDMG ENEMYEXTRADROWNDMG

      ifvarn ENEMYHASSHIKON? 0 divvar ENEMYTOTALDROWNDMG 10

      state showdamage_actor-drownhp
      subvarvar ENEMYHP ENEMYTOTALDROWNDMG
      ifrnd 192
      {
       ifrnd 128
	   {
	    stopsound SOMEONE_DROWNING1
 	    stopsound SOMEONE_DROWNING2 
	   }
       ifrnd 128 sound SOMEONE_DROWNING1 else sound SOMEONE_DROWNING2
      }

      ifvarl ENEMYHP 1
      {
       setvar ENEMYHP 0
       setactor[THISACTOR].extra 0
       state checkactorsurvivalvalue
      }
      ifvare ENEMYHP 0
      {
       stopsound SOMEONE_DROWNING1
       stopsound SOMEONE_DROWNING2
       stopsound SOMEONE_DROWNED1
       stopsound SOMEONE_DROWNED2
       ifrnd 128 globalsound SOMEONE_DROWNED1 else globalsound SOMEONE_DROWNED2
       setactor[THISACTOR].extra 0
       setactor[THISACTOR].htowner THISACTOR
       setactor[THISACTOR].htpicnum SHOTSPARK1
       setactor[THISACTOR].htextra 8000

       setvarvar tempe THISACTOR 
       ifvarl THISACTOR 16383 addvar tempe 1 else subvar tempe 1
      }
      state drownbubbles
      state drownbubbles
      setvar ENEMYDROWNTIME 0 // Reset the drown meter count to prevent cheap drown deaths
     }
    }
   }
   else // If the monster does have Scuba Tank energy
   {
    ifvare mainmenu 0 ifvare FROZEN? 0 subvar ENEMYAIRTANKAMOUNT 1  // Decrease Scuba energy by 1 per tic
   }
  }
 }
}
else // If the monster is NOT underwater
{
 setvar ENEMYBASEDROWNDMG 0
 setvar ENEMYEXTRADROWNDMG 0 // Resets the Extra Drown Damage increments
 setvar ENEMYTOTALDROWNDMG 0
 setvar ENEMYDROWNTIME 0 // Reset Drown counter
 ifvarvarl ENEMYAIRTIME ENEMYMAXAIRTIME // If the monster's airtime is not at it's maximum
 {
  setvarvar ENEMYTEMPAIR ENEMYMAXAIRTIME
  divvar ENEMYTEMPAIR 130
  addvarvar ENEMYAIRTIME ENEMYTEMPAIR // Restore air time by [Enemy's Airtime/130] per tic
 }
 ifvarvare ENEMYAIRTIME ENEMYMAXAIRTIME { setvar ENEMYAIRTIMEEXTRA 1000 }
 ifvarvarg ENEMYAIRTIME ENEMYMAXAIRTIME 
 {
  setvarvar ENEMYAIRTIME ENEMYMAXAIRTIME // If the monster's airtime goes over maximum
  setvar ENEMYAIRTIMEEXTRA 1000
 }
}
ends

