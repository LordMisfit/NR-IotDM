  ifvare temp 1 { setvar FIREACTORDMGID -1 break } // Makes it that if the current "target" is cloaked, set to -1 to search for a non-cloaked one
  state pythagoras_fire
  ifvarg FIREDISTANCE 6144 { setvar FIREACTORDMGID -1 break }
 }
ends

