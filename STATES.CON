state dealinvulndmg
     ifvarand CHEATBITFIELD? 32
     {
      soundonce INVULNITEM3
      palfrom 32 64 64 64
     }
     else
     {
      getactorvar[INVULNAURATARGET].INVULNTIME tempb
      ifvare tempb 0
      {
       state invulnauraseek
       ifvare temp 1
       {
        setvar MISCARRAYID 26 getactorvar[MISCARRAYID].MISCARRAYID_AMOUNT MISCARRAYID_AMOUNTTEMP // DEATHSWITCH-DELAY
        ifvare MISCARRAYID_AMOUNTTEMP 0
        {
         ifvare CHARACTERSELECTED? 8
         {
          ifvare HAVEENERGYSHIELD? 1
          {
           ifvare ENERGYSHIELDMODE 1
           {
            setvarvar TEMPVAR5 CURRENTBIOSHIELDPOWER
            divvarvar TEMPVAR5 MAXHITPOINTS
            ifvarg TEMPVAR5 15
            {
             setvar CURRENTCELLS 0
             setvar DAMAGEINCELL 0
             setvar FIRSTAIDCURRENT 0
             setvar CURRENTARMORPOINTS 0
             setvar CURRENTHITPOINTS 1
             setvar DEATHCHARACTERVAR 0
             setvar MISCARRAYID 26 setactorvar[MISCARRAYID].MISCARRAYID_AMOUNT 39 // DEATHSWITCH-DELAY
             state bioshieldhitsounds
             setactorvar[THISACTOR].ENEMYFIRSTAID 0
             setactorvar[THISACTOR].ENEMYAP 0
             setactorvar[THISACTOR].ENEMYHP 1
             redefinequote 321 ^23SAVED BY THE BIO-CELLULAR ENERGY SHIELD!
             setvarvar fta fta_normal setvar ftq 321
             palfrom 63 64
            }
            else state invulnkillplayer
           }
           else state invulnkillplayer
          }
          else state invulnkillplayer
         }
         else state invulnkillplayer
        }
       }
      }
      else
      {
       soundonce INVULNITEM3
       palfrom 32 64 64 64
      }
     }
ends

state invulnauradetect
ifvare mainmenu 0
{
 ifvarn INVULNAURATARGET -1
 { 
   getactor[INVULNAURATARGET].owner TEMPVAR
   // addlogvar TEMPVAR
   ifvare TEMPVAR -1 { setvar INVULNAURATARGET -1 break }

  ifvarvare INVULNAURATARGET PARENTID { setvar INVULNAURATARGET -1 break }
  getactor[INVULNAURATARGET].picnum temp
  ifvare temp APLAYER
  { 
   getactor[INVULNAURATARGET].owner TEMPVAR // Checks the owner to see if the APLAYER sprite is a hologram [-1] [this checks ensures you can't die due to your hologram being touched by an invuln carrier]
   // addlogvar TEMPVAR
   ifvare TEMPVAR -1 { setvar INVULNAURATARGET -1 break } else { getactorvar[INVULNAURATARGET].CURRENTHITPOINTS tempc }
  }
  else getactorvar[INVULNAURATARGET].ENEMYHP tempc

  ifvarl tempc 1 { setvar INVULNAURATARGET -1 break } // Makes it that if the current "seeker" is dead, set to -1 to search for a living one
  getactorvar[INVULNAURATARGET].SHRUNK? tempd
  ifvare tempd 1 { setvar INVULNAURATARGET -1 break } // Makes it that if the current "seeker" is shrunken, set to -1 to search for a non-shrunken one
  getactorvar[INVULNAURATARGET].FROZEN? tempd
  ifvare tempd 1 { setvar INVULNAURATARGET -1 break } // Makes it that if the current "seeker" is frozen, set to -1 to search for a non-frozen one
  getactorvar[INVULNAURATARGET].CLOAKED? tempd
  ifvare tempd 1 { setvar INVULNAURATARGET -1 break } // Makes it that if the current "seeker" is cloaked, set to -1 to search for a non-cloaked one

  // Attempt to inflict a killing blow if target is alive
  ifvarg tempc 0
  {
   getactor[INVULNAURATARGET].picnum tempe
   ifvarn tempe APLAYER
   {
     getactorvar[INVULNAURATARGET].SKILLSET? tempb
     ifvarn tempb 0
     {
      getactorvar[INVULNAURATARGET].ENEMYINVULNTIME tempb
      ifvare tempb 0 // If they have no Invuln, KILL 'EM! XD
      {
       state invulnauraseek
       ifvare temp 1
       {
         setactor[INVULNAURATARGET].htowner THISACTOR
         setactor[INVULNAURATARGET].htextra 1
         setactor[INVULNAURATARGET].htpicnum INVULNICON
       }
      }
      else soundonce INVULNITEM3
     }
     else
     {
      setvar INVULNAURATARGET -1
      break 
     }
   }
   else
   ifvare tempe APLAYER
   {
    getactorvar[PLAYERID].VEHICLEID tempvehicleid
    ifvare tempvehicleid -1
    {
     ifvare CHARACTERSELECTED? 10
     {
      ifvare PUBLICBETA? 0
      {
       ifrnd 1 { ifrnd 32 state dealinvulndmg }
      }
      else state dealinvulndmg
     }
     else
     ifvare CHARACTERSELECTED? 14
     {
      ifvare PUBLICBETA? 0 nullop else state dealinvulndmg
     }
     else state dealinvulndmg
    }
    else
    {
     getactorvar[tempvehicleid].ENEMYINVULNTIME tempb
     ifvare tempb 0
     {
      ifvarand CHEATBITFIELD? 4 setactorvar[tempvehicleid].ENEMYHP 1 else setactorvar[tempvehicleid].ENEMYHP 0
      setactor[tempvehicleid].htowner THISACTOR
      setactor[tempvehicleid].htextra 1
      setactor[tempvehicleid].htpicnum INVULNICON
      ifvare CHARACTERSELECTED? 10
      {
       ifvare PUBLICBETA? 0
       {
        ifrnd 1 { ifrnd 32 state dealinvulndmg }
       }
       else state dealinvulndmg
      }
      else
      ifvare CHARACTERSELECTED? 14
      {
       ifvare PUBLICBETA? 0 nullop else state dealinvulndmg
      }
      else state dealinvulndmg
     }
     else soundonce INVULNITEM3
    }
   }
  }
 }
}
ends

