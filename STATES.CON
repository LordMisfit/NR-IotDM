  ifvare temp 1 { setvar REACTORACTORDMGID -1 break } // Makes it that if the current "target" is frozen, set to -1 to search for a non-frozen one
  getactorvar[REACTORACTORDMGID].CLOAKED? temp
  ifvare temp 1 { setvar REACTORACTORDMGID -1 break } // Makes it that if the current "target" is cloaked, set to -1 to search for a non-cloaked one
