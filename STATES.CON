state pythagoras_hazard
  // next code gets distance between actortarget (x,y) and hazard (x,y), puts result in BLOODPOOLDISTANCE
  getactor[THISACTOR].x temp
  getactor[PYTHAGORASTARGET].x tempb

  subvarvar temp tempb
  mulvarvar temp temp
	
  getactor[THISACTOR].y tempc
  getactor[PYTHAGORASTARGET].y tempd

  subvarvar tempc tempd
  mulvarvar tempc tempc
	
  addvarvar temp tempc
  sqrt temp temp
  setvarvar PYTHAGORASDISTANCE temp
ends

state hazarddetectifdead
 ifvarn PYTHAGORASTARGET -1
 { 
  getactorvar[PYTHAGORASTARGET].DRIVERID temp
  ifvarn temp -1 { setvar PYTHAGORASTARGET -1 break } // For Vehicles only. If the target doesn't have a driver, the it takes damage individually.
  getactorvar[PYTHAGORASTARGET].ENEMYHP temp
  ifvarl temp 1 { setvar PYTHAGORASTARGET -1 break } // Makes it that if the current "target" is dead, set to -1 to search for a living one
  getactorvar[PYTHAGORASTARGET].SHRUNK? temp
  ifvare temp 1 { setvar PYTHAGORASTARGET -1 break } // Makes it that if the current "target" is shrunken, set to -1 to search for a non-shrunken one
  getactorvar[PYTHAGORASTARGET].FROZEN? temp
  ifvare temp 1 { setvar PYTHAGORASTARGET -1 break } // Makes it that if the current "target" is frozen, set to -1 to search for a non-frozen one
  getactorvar[PYTHAGORASTARGET].CLOAKED? temp
  ifvare temp 1 { setvar PYTHAGORASTARGET -1 break } // Makes it that if the current "target" is cloaked, set to -1 to search for a non-cloaked one
  state pythagoras_hazard
  ifvarg PYTHAGORASDISTANCE 844 { setvar PYTHAGORASTARGET -1 break }
 }
ends

