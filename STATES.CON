  getactorvar[REACTORACTORDMGID].DRIVERID temp
  ifvarn temp -1 { setvar REACTORACTORDMGID -1 break } // Vehicles only
  getactorvar[REACTORACTORDMGID].ENEMYHP temp
  ifvarl temp 1 { setvar REACTORACTORDMGID -1 break } // Makes it that if the current "target" is dead, set to -1 to search for a living one
  getactorvar[REACTORACTORDMGID].SHRUNK? temp
