     addvar RANDOMIZER 1
     ifvarg RANDOMIZER 0 ifvarl RANDOMIZER 2 mulvar TEMPVAR 5 // 1
     else ifvarg RANDOMIZER 1 ifvarl RANDOMIZER 5 mulvar TEMPVAR 4 // 2-4 +3
