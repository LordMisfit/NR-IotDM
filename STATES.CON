  getactorvar[FIREACTORDMGID].ENEMYHP temp
  ifvarl temp 1 { setvar FIREACTORDMGID -1 break } // Makes it that if the current "target" is dead, set to -1 to search for a living one
  getactorvar[FIREACTORDMGID].SHRUNK? temp
