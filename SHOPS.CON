state shop_calculatefinalprices
   ifvarg SHOPDOLLARSTEMP 21474836 setvar SHOPDOLLARSTEMP 21474836 // Prevents overflow errors with vars by limiting the maximum base-price of an item to $21,474,836
// If using a Silk Carnation [future item]
   state checksilkcarnations
   ifvarvarg PARTYSIZE ITEMARRAYID_AMOUNTTEMP setvarvar SHOPFACTORTEMP ITEMARRAYID_AMOUNTTEMP else setvarvar SHOPFACTORTEMP PARTYSIZE
   mulvar SHOPFACTORTEMP 200 // 2% per item  

   ifvarg SHOPFACTORTEMP 10200 setvar SHOPFACTORTEMP 10200 // Prevents overflow overflow errors with vars by limiting how high the factor for prices can get
   ifvarg SHOPFACTORTEMP 0
   {
    setvarvar TEMP6 SHOPDOLLARSTEMP
    setvarvar OVERFLOW_RAWVALUE_OLD TEMP6
    setvarvar OVERFLOW_TEMPMULTI SHOPFACTORTEMP
    setvarvar OVERFLOW_TEMPDIVISOR 10000
    state checkoverflow_custom
    setvarvar TEMP6 OVERFLOW_RAWVALUE_NEW
    ifvare shopmode 0 subvarvar SHOPDOLLARSTEMP TEMP6 // Buying = Decrease Buy Price
    else addvarvar SHOPDOLLARSTEMP TEMP6 // Selling = Increase Sell Price
 
    ifvarl SHOPDOLLARSTEMP 0 setvar SHOPDOLLARSTEMP 0
   }

   setvar MISCARRAYID 76 getactorvar[MISCARRAYID].MISCARRAYID_AMOUNT MISCARRAYID_AMOUNTTEMP // CARD-MEMBERSHIP?
   ifvarg MISCARRAYID_AMOUNTTEMP 0
   {
    setvarvar OVERFLOW_RAWVALUE_OLD SHOPDOLLARSTEMP
    ifvare shopmode 0
    {
     setvar MISCARRAYID 76 getactorvar[MISCARRAYID].MISCARRAYID_AMOUNT MISCARRAYID_AMOUNTTEMP // CARD-MEMBERSHIP?
     ifvare MISCARRAYID_AMOUNTTEMP 1 setvar OVERFLOW_TEMPMULTI 850
     ifvare MISCARRAYID_AMOUNTTEMP 2 setvar OVERFLOW_TEMPMULTI 700
     ifvare MISCARRAYID_AMOUNTTEMP 3 setvar OVERFLOW_TEMPMULTI 500
     ifvare MISCARRAYID_AMOUNTTEMP 4 setvar OVERFLOW_TEMPMULTI 250
     ifvarg MISCARRAYID_AMOUNTTEMP 4 setvar OVERFLOW_TEMPMULTI 10
    }
    else
    {
     setvar MISCARRAYID 76 getactorvar[MISCARRAYID].MISCARRAYID_AMOUNT MISCARRAYID_AMOUNTTEMP // CARD-MEMBERSHIP?
     ifvare MISCARRAYID_AMOUNTTEMP 1 setvar OVERFLOW_TEMPMULTI 1075
     ifvare MISCARRAYID_AMOUNTTEMP 2 setvar OVERFLOW_TEMPMULTI 1150
     ifvare MISCARRAYID_AMOUNTTEMP 3 setvar OVERFLOW_TEMPMULTI 1250
     ifvare MISCARRAYID_AMOUNTTEMP 4 setvar OVERFLOW_TEMPMULTI 1375
     ifvarg MISCARRAYID_AMOUNTTEMP 4 setvar OVERFLOW_TEMPMULTI 1495
    }
    setvarvar OVERFLOW_TEMPDIVISOR 1000
    state checkoverflow_custom
    setvarvar SHOPDOLLARSTEMP OVERFLOW_RAWVALUE_NEW

    ifvarl SHOPDOLLARSTEMP 0 setvar SHOPDOLLARSTEMP 0
   }

   ifvarvare SHOPCHEATBITTOTAL SHOPCOMBOBITS { ifvare shopmode 0 setvar SHOPDOLLARSPRICESINGLE 0 else setvarvar SHOPDOLLARSPRICESINGLE SHOPDOLLARSTEMP } else { setvarvar SHOPDOLLARSPRICESINGLE SHOPDOLLARSTEMP }
   mulvarvar SHOPDOLLARSTEMP shopamount
ends

