    ifvarg TEMPVAR4 3 state support-rand-frag1
    ifvarg TEMPVAR4 4 state support-rand-frag2
    getactorvar[CHARACTERARRAY].CHARACTERSUPPORTPAULA TEMPVAR4
    ifvarg TEMPVAR4 3 state support-rand-frag1
    ifvarg TEMPVAR4 4 state support-rand-frag2
    getactorvar[CHARACTERARRAY].CHARACTERSUPPORTMINAKO TEMPVAR4
    ifvarg TEMPVAR4 3 state support-rand-frag1
