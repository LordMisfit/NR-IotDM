//      addlogvar TEMPVAR4
      ifvare TEMPVAR4 1 { getactorvar[CHARACTERARRAY].CHARACTERMAXBREATHTIME TEMPVAR state support-lvl4-increase state support-hp-increase-notselected } // Max HP
