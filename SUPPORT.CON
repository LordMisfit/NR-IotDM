      ifvare TEMPVAR4 7 { setvarvar TEMPVAR CHARACTERSPEEDTRUE subvar TEMPVAR 46500 state support-lvl5-increase state support-speed-increase-selected } // Speed
     }
    }
   }
   else
   {
// Check Support Levels
    getactorvar[CHARACTERARRAY].CHARACTERSUPPORTDUKE TEMPVAR4
    ifvarg TEMPVAR4 3 state support-rand-frag1
    ifvarg TEMPVAR4 4 state support-rand-frag2
