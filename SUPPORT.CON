     getactorvar[CHARACTERARRAY].HPSUPPORTTEMP1 TEMPVAR2
     ifvarl TEMPVAR2 3 { ifvare TEMPVAR2 2 setvar TEMPVAR2 3 }
     setactorvar[CHARACTERARRAY].HPSUPPORTTEMP1 TEMPVAR2
ends

state support-hp-frag4
     getactorvar[CHARACTERARRAY].HPSUPPORTTEMP1 TEMPVAR2
     ifvarl TEMPVAR2 4 { ifvare TEMPVAR2 3 setvar TEMPVAR2 4 }
     setactorvar[CHARACTERARRAY].HPSUPPORTTEMP1 TEMPVAR2
ends

